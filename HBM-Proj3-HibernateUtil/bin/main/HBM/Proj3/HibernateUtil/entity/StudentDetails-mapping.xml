<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"https://hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="HBM.Proj3.HibernateUtil.entity.StudentDetails">

		<id name="stId" type="int" column="STUID">
			<generator class="increment" />
		</id>
		<property name="stName" column="STUNAME" />
		<property name="stDept" column="STUDEPT" />
		<property name="stCont" column="STUCONT" />
		<property name="stCGPA" column="CGPA" />

	</class>
</hibernate-mapping>